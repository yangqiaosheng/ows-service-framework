                            <gml:upperCorner>53.0 8.0</gml:upperCorner>
                        </gml:Envelope>
                    </sos:observedArea>
                    <sos:phenomenonTime>
                        <gml:TimePeriod gml:id="T001">
                            <gml:beginPosition>2008-01-11T00:00:00Z</gml:beginPosition>
                            <gml:endPosition>2011-08-21T00:00:00Z</gml:endPosition>
                        </gml:TimePeriod>
