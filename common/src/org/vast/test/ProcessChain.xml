<?xml version="1.0" encoding="UTF-8"?>
<sml:SensorML xmlns:sml="http://www.opengis.net/sensorML" xmlns:swe="http://www.opengis.net/swe" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xlink="http://www.w3.org/1999/xlink" xsi:schemaLocation="http://www.opengis.net/sensorML file:///D:/Projects/NSSTC/OGCSchemas/schemas_OGC/sensorML/1.0.30/base/sensorML.xsd" version="1.0">
   <sml:ProcessChain id="PROCESSING_CHAIN">
      <!--~~~~~~~~~~~~~~~~~~~-->
      <!-- Inputs Definition -->
      <!--~~~~~~~~~~~~~~~~~~~-->
      <sml:inputs>
         <sml:InputList>
            <sml:input name="temp">
               <swe:Quantity definition="urn:ogc:def:phenomenon:temperature" uom="degF"/>
            </sml:input>
         </sml:InputList>
      </sml:inputs>
      <!--~~~~~~~~~~~~~~~~~~~~-->
      <!-- Outputs Definition -->
      <!--~~~~~~~~~~~~~~~~~~~~-->
      <sml:outputs>
         <sml:OutputList>
            <sml:output name="windChillTemp1">
               <swe:Quantity definition="urn:ogc:def:phenomenon:temperature" uom="degF"/>
            </sml:output>
            <sml:output name="windChillTemp2">
               <swe:Quantity definition="urn:ogc:def:phenomenon:temperature" uom="degF"/>
            </sml:output>
         </sml:OutputList>
      </sml:outputs>
      <!--~~~~~~~~~~~~~~~~~~-->
      <!-- Sub-Process List -->
      <!--~~~~~~~~~~~~~~~~~~-->
      <sml:processes>
         <sml:ProcessList>
            <sml:process name="switchProcess">
               <sml:ProcessModel>
                  <sml:inputs>
                     <sml:InputList>
                        <sml:input name="input">
                           <swe:Quantity/>
                        </sml:input>
                        <sml:input name="treshold">
                           <swe:Quantity>5.0</swe:Quantity>
                        </sml:input>
                     </sml:InputList>
                  </sml:inputs>
                  <sml:outputs>
                     <sml:OutputList>
                        <sml:output name="less">
                           <swe:Quantity/>
                        </sml:output>
                        <sml:output name="more">
                           <swe:Quantity/>
                        </sml:output>
                        <sml:output name="equal">
                           <swe:Quantity/>
                        </sml:output>
                     </sml:OutputList>
                  </sml:outputs>
                  <sml:method xlink:href="urn:ogc:def:process:compareSwitch:1.0"/>
               </sml:ProcessModel>
            </sml:process>
            <sml:process name="windChillProcess1" xlink:href="http://vast.uah.edu/SensorML/instances/process/WindChill_Process.xml#WINDCHILL_PROCESS"/>
            <sml:process name="windChillProcess2" xlink:href="http://vast.uah.edu/SensorML/instances/process/WindChill_Process.xml#WINDCHILL_PROCESS"/>
         </sml:ProcessList>
      </sml:processes>
      <!--~~~~~~~~~~~~~-->
      <!-- Connections -->
      <!--~~~~~~~~~~~~~-->
      <sml:connections>
         <sml:ConnectionList>
            <sml:connection>
               <sml:Link>
                  <sml:source ref="this/inputs/temp"/>
                  <sml:destination ref="switchProcess/inputs/input"/>
               </sml:Link>
            </sml:connection>
            <sml:connection>
               <sml:Link>
                  <sml:source ref="switchProcess/outputs/less"/>
                  <sml:destination ref="windChillProcess1/inputs/ambiant_temperature"/>
               </sml:Link>
            </sml:connection>
            <sml:connection>
               <sml:Link>
                  <sml:source ref="switchProcess/outputs/more"/>
                  <sml:destination ref="windChillProcess2/inputs/ambiant_temperature"/>
               </sml:Link>
            </sml:connection>            
            <sml:connection>
               <sml:Link>
                  <sml:source ref="windChillProcess1/outputs/windchill_temperature"/>
                  <sml:destination ref="this/outputs/windChillTemp1"/>
               </sml:Link>
            </sml:connection>
            <sml:connection>
               <sml:Link>
                  <sml:source ref="windChillProcess2/outputs/windchill_temperature"/>
                  <sml:destination ref="this/outputs/windChillTemp2"/>
               </sml:Link>
            </sml:connection>
         </sml:ConnectionList>
      </sml:connections>
   </sml:ProcessChain>
</sml:SensorML>
